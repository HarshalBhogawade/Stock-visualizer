{
  "name": "finsight-stock-trend-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "https://harshalbhogawade.github.io/Stock-visualizer",
  "description": "FinSight: Stock Trend Dashboard - Interactive stock price visualization tool",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tailwind:build": "npx @tailwindcss/cli -i ./src/input.css -o ./src/output.css",
    "tailwind:watch": "npx @tailwindcss/cli -i ./src/input.css -o ./src/output.css --watch",
    "predeploy": "npm run tailwind:build && npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.14",
    "axios": "^1.5.0",
    "chart.js": "^4.4.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "vite": "^4.4.5"
  }
}
